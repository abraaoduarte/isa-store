{
  "name": "dashboard",
  "version": "1.0.0",
  "private": true,
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "scripts": {
    "dev": "next dev -p 6060",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:code": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:styles": "stylelint 'src/**/*.{ts,tsx,less}'",
    "lint:prettier": "yarn prettier 'src/**/*.{js,jsx,ts,tsx,less,json,md,yml}'",
    "lint:all": "yarn lint:code --max-warnings 0 && yarn lint:styles && yarn lint:prettier -c",
    "compile": "tsc --noEmit",
    "compile:all": "yarn compile --project tsconfig.json",
    "prettify": "prettier --write 'src/**/*.{js,jsx,ts,tsx,less,json,md,yml}'"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@hookform/resolvers": "^2.8.10",
    "@mui/icons-material": "^5.8.0",
    "@mui/lab": "^5.0.0-alpha.83",
    "@mui/material": "^5.8.1",
    "@mui/styles": "^5.8.0",
    "axios": "^0.27.2",
    "date-fns": "^2.28.0",
    "dinero.js": "alpha",
    "next": "12.1.6",
    "nookies": "^2.5.2",
    "notistack": "^2.0.5",
    "ramda": "^0.28.0",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-hook-form": "^7.31.2",
    "react-imask": "^6.4.2",
    "react-is": "^18.1.0",
    "react-number-format": "^4.9.3",
    "react-query": "^3.39.0",
    "slugify": "^1.6.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "17.0.35",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "8.16.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.6.2",
    "typescript": "4.6.4"
  }
}
